headers=`paste -s -d" " headers`

command="curl  -XPOST \"http://${CATALOG_FE_HOST}:${CATALOG_FE_PORT}/sdc1/rest/v1/catalog/resources\" -d \"@body.txt\" ${headers} -sL -w \":%{http_code}:%{size_request} BYTES:%{time_total} \\n\"  "

echo $command > toExec
chmod +x toExec
./toExec


